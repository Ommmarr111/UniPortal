@model List<Course>
@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>
<table class="table table-hover table-bordered">
    <thead>
        <tr>
            <th>No.</th>
            <th>Name</th>
            <th>Degree</th>
            <th>MinDegree</th>
            <th>Hours</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var course in Model)
        {
            <tr>
                <td>@course.Id</td>
                <td>@course.Name</td>
                <td>@course.Degree</td>
                <td>@course.minDegree</td>
                <td>@course.Hours</td>
            </tr>
        }
    </tbody>
</table>